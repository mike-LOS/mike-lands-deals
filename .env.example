# Get your OpenAI API Key here: https://platform.openai.com/account/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Generate a random secret: https://generate-secret.vercel.app/32 or `openssl rand -base64 32`
AUTH_SECRET=****

# Vercel Postgres Configuration (Get these from your Vercel Dashboard)
POSTGRES_URL="postgres://default:password@endpoint:5432/verceldb"
POSTGRES_PRISMA_URL="postgres://default:password@endpoint:5432/verceldb?pgbouncer=true&connect_timeout=15"
POSTGRES_URL_NON_POOLING="postgres://default:password@endpoint:5432/verceldb"
POSTGRES_USER="default"
POSTGRES_HOST="endpoint"
POSTGRES_PASSWORD="password"
POSTGRES_DATABASE="verceldb"

# Privy Configuration
NEXT_PUBLIC_PRIVY_APP_ID=your_privy_app_id_here
NEXT_PUBLIC_PRIVY_CLIENT_ID=your_privy_client_id_here
PRIVY_SECRET_KEY=your_privy_secret_key_here
PRIVY_JWKS_URL=https://auth.privy.io/v1/apps/${NEXT_PUBLIC_PRIVY_APP_ID}/jwks
NEXT_PUBLIC_APP_URL=http://localhost:3000

# The following keys below are automatically created and
# added to your environment when you deploy on vercel

# Instructions to create a Vercel Blob Store here: https://vercel.com/docs/storage/vercel-blob
BLOB_READ_WRITE_TOKEN=****

# Add any other environment variables your app needs with placeholder values
